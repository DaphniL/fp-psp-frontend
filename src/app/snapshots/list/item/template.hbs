<li class="list-group-item">

  <div class="header-indicators-snapshot">
    <h3 class="pull-left"> SNAPSHOT {{ header.date }} </h3>
    <div class="pull-right header-resume">
      <p>{{ header.data.count_red_indicators}}</p>
      <div class="circle-header red " />
      <p>{{ header.data.count_yellow_indicators}}</p>
      <div class="circle-header yellow " />
      <p>{{ header.data.count_green_indicators}}</p>
      <div class="circle-header green " />
    </div>

  </div>
  <hr />
  <div>
    {{#each header.extra}}
    <label>{{this.label}}:</label> {{this.value}} {{/each}}
  </div>
  <hr />
  <hr /> 

  <div class="row">
    {{#each data}}
    <div class="col-md-2 col-lg-1 col-sm-2 col-xs-4 col-xm-2 indicator-circle-snapshot">
      <div id="circle" class="circle-content {{ this.clazz }}" />
      <p id="indicator-name">{{ this.name }}</p>
      <p hidden id="indicator-value">{{ this.value }}</p>
    </div>
    {{/each}}


  </div>


  <br/>
  <br/>
  <table class="table table-hover margin bottom center" id="prioritiesTable">
    <thead class="thead-light">
      <tr>
        <th>Indicator</th>
        <th>Why? I do not have it?</th>
        <th>What to do? to have it?</th>
        <th>When you achieve it?</th>
      </tr>
    </thead>
    <tbody id="tbodypriorities">
      {{#each priorities}} 
      <tr>
        <td> {{this.indicator}}</td>
        <td> {{this.reason}}</td>
        <td> {{this.action}}</td>
        <td> {{this.estimated_date}}</td>
        <td> <button class="btn btn-danger" data-id="{{ this.snapshot_indicator_priority_id }}">Borrar</button></td>
      </tr>
      {{/each}}

    </tbody>
  </table>


</li>

<div id="modal-region">
</div>